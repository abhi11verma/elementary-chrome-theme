{
    "name":"Elementary Chrome Theme",
    "version": "0.1",
    "description": "elementary OS Loki Chrome Theme",
    "manifest_version": 1,
    "theme": {
        "images": {
            "theme_frame": "images\/theme_frame.png"
        },
        "colors": {
            "frame": [222,222,222],
            "toolbar": [223,223,223],
            "tab_text": [0,0,0],
            "tab_background_text": [64,64,64],
            "bookmark_text": [92,91,91],
            "ntp_background": [255,255,255],
            "ntp_text":[0,0,0],
